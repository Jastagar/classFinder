<div class="col-md-4 p-1">
<button type="button" style='text-transform: capitalize;' class="btn btn-primary w-100" data-bs-toggle="modal" data-bs-target="#exampleModal<%= dataArr.class %>">
    <%= dataArr.class%>
</button>
  <div class="modal fade modal-xl" id="exampleModal<%= dataArr.class %>" tabindex="-1" aria-labelledby="exampleModalLabel<%= dataArr.class %>" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
            <img class="w-100" style="height: 70vh;" src="/img/<%= dataArr.class %>.png">
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
          <button type="button" class="btn btn-primary">Save changes</button>
        </div>
      </div>
    </div>
  </div>
  </div>